package com.baeldung.multipledb.model;

public class Demo {

	 int rollno;  
	 String name;  
	 String city;
	public Demo(int rollno, String name, String city) {
		super();
		this.rollno = rollno;
		this.name = name;
		this.city = city;
	}  

	Demo d1=new Demo(1,"Dikshant","mandi");
	Demo d2 = new Demo(2,"abc","xyz");
	
}

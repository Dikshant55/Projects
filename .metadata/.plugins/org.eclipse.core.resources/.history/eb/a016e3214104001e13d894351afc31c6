package com.practice.java;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collector;
import java.util.stream.Collectors;

public class DemoListOperations {
	
	public static void main(String[] args) {
		
		//creating list (Arrays.asList-return a fixed-size list)
		List<Integer> number = Arrays.asList(23,34,54,32,21);
		
		List<String> names = Arrays.asList("Dikshant","Sharma","Preet","Bharwal");
		
		List<String> result= names.stream().filter(s -> s.startsWith("P"))
				                           .collect(Collectors.toList());
		System.out.println(result);
	}

}
